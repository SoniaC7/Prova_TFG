\t\t<div class=\"column\">\n\t\t\t\t\t\t<img id=\"realIdMedium\" :src=\"this.real_image\" v-on:click=\"increment_score_lose(index)\"/>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div class=\"column\" >\n\t\t\t\t\t\t<img id=\"mediaIdMedium\" :src=\"this.media_image\" v-on:click=\"increment_score_win(index)\"/>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"ma-2\" >\n          <h4> Accuracy </h4>\n          <div> {{ accuracy_value }}% </div>\n\t\t\t\t\t<v-progress-linear\n            color=\"green\"\n            v-model=\"accuracy_value\"\n            height=\"10\"\n            striped\n            rounded\n            style=\"border: 1px solid black;\"\n          ></v-progress-linear>\n          <!--<div class=\"progress-bar\">\n\t\t\t\t\t\t<div class=\"percentatge\" :style=\"{'width' : progress + '%'}\"></div>\n\t\t\t\t\t</div>-->\n\t\t\t\t</div>\n\n\t\t\t\t<br>\n\t\t\t\t<div class=\"btn-section\">\n\t\t\t\t\t<v-btn v-bind:disabled=\"!image_clicked\" v-bind=\"buttonsTheme\" v-on:click=\"next()\" > Next </v-btn>\n\t\t\t\t</div>\n\n\t\t</div>\n\t\t\t<div v-show=\"this.finish_level\">\n\t\t\t\t<br>\n\t\t\t\t<h2> Congrats! You have finished the level </h2>\n\t\t\t\t<div v-show=\"this.badge_earned\">\n\t\t\t\t\t<h2> You earned a new badge! </h2>\n\t\t\t\t\t<div class=\"img-section\">\n\t\t\t\t\t\t<img :src=\"badges[0].example\" >\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<v-row align=\"center\">\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n          >\n              <v-btn-toggle style=\"flex-direction: column; width: 60%;\" background-color=\"rgb(255, 0, 0, 0)\">\n              <v-btn style=\"font-size: 20px\" v-bind=\"buttonsTheme\" class=\"my-6\" v-for=\"section in sections\" :to=\"section.to\" :key=\"section.title\" v-on:click=\"level_completed_method()\">{{ section.title }}</v-btn>\n              </v-btn-toggle>\n          </v-col>\n        </v-row> \n\t\t  </div> \n</div>\n</template> \n<style scoped>\n    .column img {\n      width: 108px;\n      height: 143px;\n      margin: 0 auto;\n      border-radius: 5% !important;\n      overflow: hidden;\n    }\n    .column{\n      float: left;\n      width: 50%;\n      padding: 5px;\n      /*border: 3px solid blue;*/\n    }\n\n    .img-section::after{\n      content: \"\";\n      clear: both;\n      display: table; \n    }\n</style>\n\n<script>\n  export default {\n    name:'Medium',\n    data: () => ({ \n      index: 1,\n      i_clicked: 1,\n      total_images: 15,\n      fol_name: '',\n      score: 0,\n      progress: 0,\n      accuracy:0,\n      num_correct_answers:0,\n      total_answers:0,\n      finish_level: false,\n      badge_earned: false,\n      msg_win: false,\n      msg_lose: false,\n      media_right: false,\n      image_clicked:false,\n\n      media_image: require('../assets/images/media/medium/1/media.png'),\n      real_image: require('../assets/images/media/medium/1/real.png'),\n\n      sections: [\n        { title: ' Home ', to:'/'},\n        { title: ' Game ', to:'/game'}\n      ] \n    }),\n    computed:{\n      /*progressed(){\n          return this.progress.toFixed();\n      },*/\n      accuracy_value(){\n          return this.accuracy.toFixed();\n      },\n      buttonsTheme(){\n          return this.$store.state.buttonsTheme;\n      },\n      badges(){\n            return this.$store.state.badges;\n      }\n    }, \n    \n    methods:{\n        log(message){\n            console.log(message);\n        },\n        reset_values(){\n          this.index = 1;\n          this.i_clicked= 1;\n          this.total_images= 15;\n          this.fol_name= '';\n          this.score= 0;\n          this.progress= 0;\n          this.accuracy= 0;\n          this.total_answers= 0;\n          this.num_correct_answers= 0;\n          this.finish_level= false;\n          this.badge_earned= false;\n          this.msg_win= false;\n          this.msg_lose= false;\n          this.media_right= false;\n          this.image_clicked= false;\n\n          this.media_image= require('../assets/images/media/medium/1/media.png');\n          this.real_image= require('../assets/images/media/medium/1/real.png');\n        },\n        show_badges(){\n            this.$store.commit('show_badges');\n        },\n        badge_earned_method(id){\n            this.$store.commit('badge_earned',id);\n        },\n        level_completed_method(){\n          this.reset_values();\n          this.$store.commit('level_completed');\n          this.$parent.reset_values();\n          \n          //this.$store.commit('reset_level_completed');\n        },\n        add_border(media_clicked){\n          if(media_clicked)document.getElementById(\"mediaIdMedium\").style.border = \"2px solid black\" ;\n          else document.getElementById(\"realIdMedium\").style.border = \"2px solid black\";\n        },\n        remove_borders(){\n          document.getElementById(\"mediaIdMedium\").style.border = \"0px solid black\";\n          document.getElementById(\"realIdMedium\").style.border = \"0px solid black\";\n        },\n        increment_score_win(index){\n            if( this.i_clicked == index){\n                this.score = this.score + 2;\n                this.msg_win = true;\n                this.num_correct_answers += 1;\n                this.i_clicked += 1;\n                this.add_border(true);\n                this.increment_progress();\n                this.image_clicked = true;\n            }\n        },\n        increment_score_lose(index){\n            if( this.i_clicked == index){\n                this.score = this.score + 1;\n                this.msg_lose = true;\n                this.i_clicked += 1;\n                this.add_border(false);\n                this.increment_progress();\n                this.image_clicked = true;\n            }\n        },\n        increment_progress(){\n          this.total_answers += 1;\n          this.set_accuracy();\n            if (this.progress < 100)\n                this.progress = Math.round((this.score / this.total_images /2) * 100);\n            \n        },\n        set_accuracy(){\n          this.accuracy = Math.round((this.num_correct_answers / this.total_answers ) * 100);\n        },\n        getImageMedia(index){\n            var in_string = index.toString();\n            this.fol_name = require('../assets/images/media/medium/'+ in_string + '/media.png');\n            return this.fol_name;\n        },\n        getImageReal(index){\n            var in_string = index.toString();\n            this.fol_name = require('../assets/images/media/medium/'+ in_string + '/real.png');\n            return this.fol_name;\n        },\n        next(){\n            \n            this.remove_borders();\n            this.image_clicked = false;\n            this.index = this.index + 1;\n            this.log(this.index);\n            this.log(this.score);\n            \n            this.msg_win = false;\n            this.msg_lose = false;\n            \n            this.media_right= Math.random() < 0.5;\n            this.log(this.media_right);\n\n            if(this.index <= this.total_images){\n                this.media_image = this.getImageMedia(this.index);\n                this.real_image = this.getImageReal(this.index);\n            }\n            else{\n                this.finish_level = true;\n                /*if(this.progress == 100){\n                    this.badge_earned = true;\n                    this.badge_earned_method(0);\n                }*/\n            }\n        }\n    }\n  }\n</script>"]}]}