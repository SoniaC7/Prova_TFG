AA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,MAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,EAAA;AAAA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AAHA,OATA,EAaA;AACA,QAAA,EAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA,mCAFA;AAGA,QAAA,SAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,EAAA;AAAA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AAHA,OAbA,EAiBA;AACA,QAAA,EAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA,qEAFA;AAGA,QAAA,SAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,OAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,EAAA;AAAA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AAHA,OAjBA,CADA;AAwBA,MAAA,aAAA,EAAA,CAxBA;AAyBA,MAAA,WAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,KAAA,CAzBA;AA0BA,MAAA,QAAA,EAAA,CACA;AAAA,QAAA,KAAA,EAAA,MAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,KAAA,EAAA,MAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,KAAA,EAAA,MAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAHA;AA1BA,KAAA;AAAA,GAFA;AAkCA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,aAAA,KAAA,WAAA,CAAA,MAAA,CAAA,UAAA,GAAA,EAAA;AAAA,eAAA,GAAA;AAAA,OAAA,EAAA,MAAA,GAAA,CAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,YAAA;AACA;AANA,GAlCA;AA0CA,EAAA,OAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,WAAA,aAAA;AACA;AAHA;AA1CA,CAAA","sourcesContent":["<template>\n  <div class=\"quiz\">\n    <h1 class=\"mb-3 mt-3\"> Quiz </h1>\n\t\t\t<div v-for=\"(question,index) in questions\" :key=\"question.id\">\n\t\t\t\t<div v-show=\"index === questionIndex\">\n\t\t\t\t\t<h2 style=\"font-size:18px\">{{ question.text }}</h2>\n\t\t\t\t\t\n          <ol style=\"list-style-type: none\">\n\t\t\t\t\t\t<li class=\"mt-6  mr-6 mb-6\" style=\"font-size:25px\" v-for=\"response in question.responses\" :key=\"response.id_r\">\n\t\t\t\t\t\t\t<label>\n\t\t\t\t\t\t\t\t<input type=\"radio\" \n\t\t\t\t\t\t\t\t\tv-bind:value=\"response.correct\" \n\t\t\t\t\t\t\t\t\tv-bind:name=\"index\" \n\t\t\t\t\t\t\t\t\tv-model=\"u_responses[index]\"\n                  > {{response.text}}\n\t\t\t\t\t\t\t</label>\n\t\t\t\t\t\t</li>\n\t\t\t\t\t</ol>\n          <v-btn style=\"font-size: 15px\" v-bind=\"buttonsTheme\" v-on:click=\"next\"> Next question </v-btn>\n\n          <v-btn style=\"font-size: 10px\" v-bind=\"buttonsTheme\" class=\"ma-10\" :to=\"sections[2].to\"> MENU </v-btn>\n\t\t\t\t\t\t\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div v-show=\"questionIndex === questions.length\">\n\t\t\t\t<h2 class=\"mb-6 mt-6\">Congrats! You finished the quiz</h2>\n\t\t\t\t\t<div v-if=\"total_score< 1\">\n\t\t\t\t\t\t<h2 class=\"mb-10 mt-10\"> Here's some tips you can use to identify fake photos</h2>\n            <v-btn style=\"font-size: 15px\" v-bind=\"buttonsTheme\" :to=\"sections[0].to\"> {{ sections[0].title }}</v-btn>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div v-else>\n\t\t\t\t\t\t<h2 class=\"mb-6 mt-6\">Let's play!</h2>\n            <v-btn style=\"font-size: 15px\" v-bind=\"buttonsTheme\" :to=\"sections[1].to\"> {{ sections[1].title }}</v-btn>\n\t\t\t\t\t</div>\n\t\t\t\t\n\t\t\t\t<!--<p>\n\t\t\t\t\tTotal score: {{ total_score }} \n\t\t\t\t</p>-->\n\t\t\t</div>\n  </div>\n</template>\n<script>\n  export default {\n    name:'Quiz',\n    data: () => ({ \n      questions: [\n        {\n            id:0,\n            text: \"When you see a photo do you go to the users' profile?\",\n            responses: [{id_r:0,text: \"Yes\", correct: true}, {id_r:1,text:\"No\"}]\n        },{\n            id:1,\n            text: \"How many seconds do you spend on each photo?\",\n            responses: [{id_r:2,text: \"< 5s\"}, {id_r:3,text:\"> 5s\", correct: true}]\n        },{\n            id:2,\n            text: \"How many hours do you spend on social media in a day?\",\n            responses: [{id_r:4,text: \"< 4h\", correct: true}, {id_r:5,text:\"> 4h\"}]\n        },{\n            id:3,\n            text: \"Are you satisfied with your body?\",\n            responses: [{id_r:6,text: \"Yes\", correct: true}, {id_r:7,text:\"No\"}]\n        },{\n            id:4,\n            text: \"From all the people you follow, how many are models or influencers?\",\n            responses: [{id_r:8,text: \"< 50%\", correct: true}, {id_r:9,text:\"> 50%\"}]\n        }\n      ],\n      questionIndex: 0,\n      u_responses: Array(5).fill(false),\n      sections: [\n        { title: 'Tips', to:'/tips'},\n        { title: 'Game', to:'/game'},\n        { title: 'Home', to:'/'}\n        ]\n    }),\n    computed:{\n        total_score(){\n            return this.u_responses.filter(function(val){return val}).length / 5;\n        },\n        buttonsTheme(){\n          return this.$store.state.buttonsTheme;\n        }\n    },\n    methods:{\n        next: function() {\n            this.questionIndex++;\n        }\n    }\n  }\n</script>"],"sourceRoot":"src/views"}]}